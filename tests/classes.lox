class Hobbit {
    class init() {
        print "Welcome to the shire!";
    }

    init(name, surname) {
        print name + " " + surname + " was born!";
        this.name = name;
        this.surname = surname;
    }

    formalName {
        return "Mr " + this.surname;
    }

    class spawn(clan, index) {
        print "A newborn in the Shire!";
        return Hobbit("Hobbit" + index, clan);
    }
}

